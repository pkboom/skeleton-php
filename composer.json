{
    "name": "pkboom/:package_name",
    "description": ":package_description",
    "keywords": [
        "pkboom",
        ":package_name"
    ],
    "homepage": "https://github.com/pkboom/:package_name",
    "license": "MIT",
    "authors": [
        {
            "name": ":author_name",
            "email": ":author_email",
            "homepage": "https://pkboom.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "symfony/var-dumper": "^4.2",
        "phpunit/phpunit": "^7.0"
    },
    "autoload": {
        "psr-4": {
            "Pkboom\\Skeleton\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Pkboom\\Skeleton\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Pkboom\\Skeleton\\SkeletonServiceProvider"
            ],
            "aliases": {
                "Skeleton": "Pkboom\\Skeleton\\SkeletonFacade"
            }
        }
    }
}